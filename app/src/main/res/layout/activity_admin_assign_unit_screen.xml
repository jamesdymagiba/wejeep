<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminAssignUnitScreen">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/blueTextColor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="306dp"
        android:layout_height="38dp"
        android:fontFamily="@font/londrina_solid_regular"
        android:text="@string/assign_unit"
        android:textColor="@color/white"
        android:textSize="25sp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar_1"
        app:layout_constraintEnd_toEndOf="@+id/toolbar_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar_1" />

    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:id="@+id/imgview_1"
        android:layout_width="380dp"
        android:layout_height="410dp"
        android:layout_marginBottom="144dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_1"
        app:layout_constraintVertical_bias="0.555"
        app:srcCompat="@drawable/rectangle_white_bg"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/londrina_solid_regular"
        android:text="@string/unit_no"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imgview_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toStartOf="@+id/imgview_1"
        app:layout_constraintTop_toTopOf="@+id/imgview_1"
        app:layout_constraintVertical_bias="0.025" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/londrina_solid_regular"
        android:text="From Day:"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imgview_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.122"
        app:layout_constraintStart_toStartOf="@+id/imgview_1"
        app:layout_constraintTop_toTopOf="@+id/imgview_1"
        app:layout_constraintVertical_bias="0.689" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/londrina_solid_regular"
        android:text="Conductor:"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imgview_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toStartOf="@+id/imgview_1"
        app:layout_constraintTop_toTopOf="@+id/imgview_1"
        app:layout_constraintVertical_bias="0.493" />

    <Spinner
        android:id="@+id/spinnerDriver"
        android:layout_width="270dp"
        android:layout_height="45dp"
        android:background="@drawable/rounded_edit_text"
        app:layout_constraintBottom_toBottomOf="@+id/imgview_1"
        app:layout_constraintEnd_toEndOf="@+id/imgview_1"
        app:layout_constraintHorizontal_bias="0.381"
        app:layout_constraintStart_toStartOf="@+id/imgview_1"
        app:layout_constraintTop_toTopOf="@+id/imgview_1"
        app:layout_constraintVertical_bias="0.41" />

    <Spinner
        android:id="@+id/spinnerConductor"
        android:layout_width="270dp"
        android:layout_height="45dp"
        android:background="@drawable/rounded_edit_text"
        app:layout_constraintBottom_toBottomOf="@+id/imgview_1"
        app:layout_constraintEnd_toEndOf="@+id/imgview_1"
        app:layout_constraintHorizontal_bias="0.381"
        app:layout_constraintStart_toStartOf="@+id/imgview_1"
        app:layout_constraintTop_toTopOf="@+id/imgview_1"
        app:layout_constraintVertical_bias="0.578" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/londrina_solid_regular"
        android:text="@string/plate_no"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imgview_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.117"
